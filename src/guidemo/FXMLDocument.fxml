<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="guidemo.FXMLDocumentController">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="234.0" prefWidth="175.0" spacing="10.0">
         <children>
            <Label text="CheckBox Objects">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <CheckBox fx:id="pineappleCheckBox" mnemonicParsing="false" text="Pineapple" />
            <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperoni" />
            <CheckBox fx:id="baconCheckBox" mnemonicParsing="false" text="Bacon" />
            <Button mnemonicParsing="false" onAction="#pizzaOrderButtunPushed" text="Order Pizza" />
            <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" prefHeight="83.0" prefWidth="163.0" text="Label" />
         </children>
      </VBox>
   </children>
</AnchorPane>
